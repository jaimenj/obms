{% extends 'AppBundle::layout.html.twig' %}

{% block mainarea -%}

<h1>Down</h1>

<div class="table-responsive">
	<table class="table table-striped table-hover">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ entity.id }}</td>
			</tr>
			<tr>
				<th>Initdate</th>
				<td>{{ entity.initdate|date('Y-m-d H:i:s') }}</td>
			</tr>
			<tr>
				<th>Finishdate</th>
				<td>{{ entity.finishdate|date('Y-m-d H:i:s') }}</td>
			</tr>
		</tbody>
	</table>
</div>

<a class="btn btn-default" href="{{ path('workerdown') }}">
	Back to the list
</a>
<a class="btn btn-default" href="{{ path('workerdown_edit', { 'id': entity.id }) }}">
	Edit
</a>

{{ form(delete_form) }}

{% endblock %}
