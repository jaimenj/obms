{% extends 'AppBundle::layout.html.twig' %}

{% block mainarea -%}

<h1>Third type</h1>

<div class="table-responsive">
	<table class="table table-striped table-hover">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ entity.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ entity.name }}</td>
			</tr>
		</tbody>
	</table>
</div>

<a class="btn btn-default" href="{{ path('thirdtype') }}">
	Back to the list
</a>
<a class="btn btn-default" href="{{ path('thirdtype_edit', { 'id': entity.id }) }}">
	Edit
</a>
{{ form(delete_form) }}

{% endblock %}
