{% extends 'AppBundle::layout.html.twig' %}

{% block mainarea -%}

<h1>Third</h1>

<div class="table-responsive">
	<table class="table table-striped table-hover">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ entity.id }}</td>
			</tr>
			<tr>
				<th>Fullname</th>
				<td>{{ entity.fullname }}</td>
			</tr>
			<tr>
				<th>Type</th>
				<td>{{ entity.thirdtype }}</td>
			</tr>
			<tr>
				<th>Telephone</th>
				<td>{{ entity.telephone }}</td>
			</tr>
			<tr>
				<th>Address</th>
				<td>{{ entity.address }}</td>
			</tr>
			<tr>
				<th>Email</th>
				<td>{{ entity.email }}</td>
			</tr>
			<tr>
				<th>Web</th>
				<td>{{ entity.web }}</td>
			</tr>
		</tbody>
	</table>
</div>

<a class="btn btn-default" href="{{ path('third') }}">
	Back to the list
</a>
<a class="btn btn-default" href="{{ path('third_edit', { 'id': entity.id }) }}">
	Edit
</a>
{{ form(delete_form) }}

{% endblock %}
